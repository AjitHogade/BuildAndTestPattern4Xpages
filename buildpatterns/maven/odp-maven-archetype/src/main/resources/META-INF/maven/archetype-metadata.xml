<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor name="odp-maven-archetype">
	<requiredProperties>
		<requiredProperty key="version">
			<defaultValue>1.0.0-SNAPSHOT</defaultValue>
		</requiredProperty>
	</requiredProperties>
	<fileSets>
		<fileSet encoding="UTF-8" filtered="true">
			<directory></directory>
			<includes>
				<include>*.md</include>
			</includes>
		</fileSet>
	</fileSets>
	<modules>
		<module id="${rootArtifactId}.odp" dir="__rootArtifactId__.odp"
			name="${rootArtifactId}.odp">
			<fileSets>
				<fileSet encoding="UTF-8">
					<directory>on-disk-project</directory>
				</fileSet>
			</fileSets>
		</module>
	</modules>
</archetype-descriptor>